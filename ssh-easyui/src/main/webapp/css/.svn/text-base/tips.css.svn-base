.tips{
	display:none;
	margin-left:40px;
	position:absolute;
	z-index:199;
	padding:5px 5px 5px;
	background:#555;
	color:#fdfdfd;
	font-size:12px;
	filter:alpha(opacity=90);
	opacity:0.9;
	max-width:350px;
}
.tips .highlight,.sdk-tips .highlight{
	color:#4BD8DF;
}
.tips .corner{
	  position: absolute;
	  height:0px;
	  left: -10px;
	  top: 10px;
	  width:0px;
	  overflow: hidden;
	  font-size: 0;
	  line-height: 0;
	  border-color: transparent #555 transparent transparent;
	  border-style: dashed solid dashed dashed;
	  border-width: 5px;
}
.help{
  background:url(../images/nom.png) no-repeat 0 0px;
  height: 24px;
  width: 25px;
  border: 0
}
.help:hover{
  background:url(../images/nom-ed.png) no-repeat 0px 0px;
}